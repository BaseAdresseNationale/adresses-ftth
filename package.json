{
  "name": "@etalab/adresses-ftth",
  "version": "0.0.0",
  "description": "Extraction des adresses contenues dans les fichiers de déploiement FTTH",
  "main": "index.js",
  "repository": "git@github.com:etalab/adresses-ftth.git",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "download-contours": "curl http://etalab-datasets.geo.data.gouv.fr/contours-administratifs/2019/geojson/communes-5m.geojson.gz | gunzip > communes-5m.json",
    "preload": "node --max-old-space-size=8192 bin/preload",
    "build": "node --max-old-space-size=8192 bin/build",
    "clean:db": "rm -f .tmp.sqlite",
    "clean:dist": "rm -rf dist/*",
    "clean:contours": "rm -rf communes-5m.json",
    "clean": "yarn clean:db && yarn clean:dist && yarn clean:contours",
    "lint": "xo",
    "test": "ava"
  },
  "dependencies": {
    "@etalab/adresses-util": "^0.1.0",
    "@etalab/decoupage-administratif": "^0.6.2",
    "@keyv/sqlite": "^2.0.0",
    "@turf/turf": "^5.1.6",
    "bluebird": "^3.5.5",
    "codes-postaux": "^3.0.0",
    "csv-parser": "^2.3.0",
    "dotenv": "^8.0.0",
    "end-of-stream": "^1.4.1",
    "fs-extra": "^8.0.1",
    "geojson-stream": "^0.1.0",
    "get-stream": "^5.1.0",
    "gunzip-stream": "^1.0.1",
    "keyv": "^3.1.0",
    "lodash": "^4.17.11",
    "pumpify": "^2.0.0",
    "talisman": "^0.21.0",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2
  }
}
